#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules/next/dist/bin/node_modules:/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules/next/dist/node_modules:/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules/next/node_modules:/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules:/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules/next/dist/bin/node_modules:/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules/next/dist/node_modules:/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules/next/node_modules:/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules:/mnt/c/Users/start/Documents/Anil_237_new/HolidayDeals Site Monorepo/Github/monorepo-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../../../next@15.4.2_react-dom@19.1._fb8b5aa738f7ddcb8c1df0f3204f5b73/node_modules/next/dist/bin/next" "$@"
fi
